            <xml-property name="queryText"><![CDATA[SELECT
	b.x_code,
	b.x_code || '-' || b. NAME location_name,
	C .default_code,
	D."name" ten_sp,
	A .opening_location,
	A .closing_location,
	A .out_location,
	A .in_location,
	G.name donvi,
	F.name ma_lot,
	to_char(F.life_date, 'dd/mm/yyyy') life_date
FROM
	rpt_stock_location_in_out_line A
LEFT JOIN stock_location b ON b. ID = A .location_id
LEFT JOIN product_product C ON C . ID = A .product_id
LEFT JOIN product_template D ON D. ID = C .product_tmpl_id
LEFT JOIN stock_production_lot F ON F. ID = A .lot_id
LEFT JOIN uom_uom G ON G. ID = A .uom_id
where d.type != 'consu' and d.x_card_type = 'none'
ORDER BY
	b.x_code,
	b."name"]]></xml-property>
