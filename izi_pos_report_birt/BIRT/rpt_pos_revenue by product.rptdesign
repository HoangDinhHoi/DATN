            <xml-property name="queryText"><![CDATA[SELECT
	a.ten_chi_nhanh,
	a.ma_chi_nhanh,
	a.cong_dung,
	a.ma_nhom_sp,
	a.ma_sp,
	a.ten_sp,
	SUM(a.qty) AS so_luong,
	SUM(a.doanh_so) AS doanh_so,
	SUM(ROUND(a.dt_net)) AS doanh_thu_net,
	SUM(a.ck_cp) AS ck_cp,
	SUM(ROUND(a.dt_sau_gg)) AS dt_sau_gg
FROM
	(SELECT
		rb.name ten_chi_nhanh,
		rb.code ma_chi_nhanh,
		(SELECT name FROM product_features WHERE id=pt.x_feature_product) AS cong_dung,
		(SELECT x_code FROM product_category WHERE id=pt.categ_id) AS ma_nhom_sp,
		pt.default_code AS ma_sp,
		pt.name AS ten_sp,
		pol.qty,
		po.name,
		pol.x_revenue_rate,
		(pol.price_unit * pol.qty) AS doanh_so,
		(CASE WHEN po.pos_reference LIKE 'ES%' THEN pol.price_subtotal_incl ELSE COALESCE(dt_net.amount*pol.x_revenue_rate, 0) END) AS dt_net,
		COALESCE(ck_cp.amount*pol.x_revenue_rate, 0) AS ck_cp,
		(CASE WHEN po.pos_reference LIKE 'ES%' THEN pol.price_subtotal_incl ELSE COALESCE(dt_sau_gg.amount*pol.x_revenue_rate, 0) END) AS dt_sau_gg
	FROM
		pos_order AS po
		LEFT JOIN res_branch AS rb ON (rb.id=po.branch_id)
		LEFT JOIN pos_order_line AS pol ON (pol.order_id=po.id)
		LEFT JOIN product_product AS pp ON (pp.id=pol.product_id)
		LEFT JOIN (
			SELECT
				id, categ_id, x_feature_product, name, default_code, type
			FROM 
				product_template
		) AS pt ON (pt.id=pp.product_tmpl_id) 
		LEFT JOIN (
			SELECT
				po.id AS id,
				SUM(al.amount) AS amount
			FROM
				pos_order AS po
			JOIN
				account_bank_statement_line al ON (al.pos_statement_id=po.id)
			JOIN
				account_journal AS aj ON (al.journal_id=aj.id)
			GROUP BY
				po.id
		) AS dt_sau_gg ON (dt_sau_gg.id=po.id) -- Lay ra doanh thu sau CK
		LEFT JOIN (
			SELECT
				po.id AS id, 
				SUM(al.amount) AS amount
			FROM
				pos_order AS po
			JOIN
				account_bank_statement_line al ON (al.pos_statement_id=po.id)
			JOIN
				account_journal AS aj ON (al.journal_id=aj.id)
			WHERE
				aj.type != 'coupon'
			GROUP BY
				po.id
		) AS dt_net ON (dt_net.id=po.id)   -- Lay ra doanh thu net
		LEFT JOIN (
			SELECT
				po.id AS id,
				SUM(al.amount) AS amount
			FROM
				pos_order AS po
			JOIN
				account_bank_statement_line al ON (al.pos_statement_id=po.id)
			JOIN
				account_journal AS aj ON (al.journal_id=aj.id)
			WHERE
				aj.type = 'coupon'
			GROUP BY
				po.id
		) AS ck_cp ON (ck_cp.id=po.id)  -- Lay ra CK coupon/voucher
	WHERE
		(rb.id = ANY(string_to_array(?, ',') :: INTEGER[] ) or ?='0')
		AND po.state IN ('invoiced', 'paid', 'done')
		AND (po.date_order + INTERVAL '7 hours')::DATE >= to_date(?, 'dd/mm/yyyy')
		AND (po.date_order + INTERVAL '7 hours')::DATE <= to_date(?, 'dd/mm/yyyy')
		
	UNION ALL
	
	SELECT
		rb.name ten_chi_nhanh,
		rb.code ma_chi_nhanh,
		(SELECT name FROM product_features WHERE id=pt.x_feature_product) AS cong_dung,
		(SELECT x_code FROM product_category WHERE id=pt.categ_id) AS ma_nhom_sp,
		pt.default_code AS ma_sp,
		pt.name AS ten_sp,
		sol.product_uom_qty,
		so.name,
		0,
		round(sol.product_uom_qty * sol.price_unit, 2) AS doanh_so,
		round(sol.product_uom_qty * sol.price_unit, 2) AS dt_net,
		0 AS ck_cp,
		round(sol.product_uom_qty * sol.price_unit, 2) AS dt_sau_gg
		FROM
			sale_order AS so
		LEFT JOIN res_branch AS rb ON (rb.id=so.branch_id)	
		LEFT JOIN sale_order_line AS sol ON (sol.order_id=so.id)
		LEFT JOIN product_product AS pp ON (pp.id=sol.product_id)
		LEFT JOIN (
			SELECT
				id, categ_id, x_feature_product, name, default_code, type
			FROM 
				product_template
		) AS pt ON (pt.id=pp.product_tmpl_id) 
	WHERE
		(rb.id = ANY(string_to_array(?, ',') :: INTEGER[] ) or ?='0')
		AND so.state IN ('sale', 'done')
		AND (so.confirmation_date + INTERVAL '7 hours')::DATE  >= to_date(?, 'dd/mm/yyyy')
		AND (so.confirmation_date + INTERVAL '7 hours')::DATE <= to_date(?, 'dd/mm/yyyy')
	) AS a
	
	GROUP BY
		a.ten_chi_nhanh, a.cong_dung, a.ma_nhom_sp, a.ma_sp, a.ten_sp, a.ma_chi_nhanh
	ORDER BY
		a.ma_sp
]]></xml-property>
