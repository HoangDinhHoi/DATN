            <xml-property name="queryText"><![CDATA[-- HoiHD
SELECT
	rb.name ten_chi_nhanh,
	rb.code ma_chi_nhanh,
	resp.name AS ten_kh,
	resp.x_partner_code AS ma_kh,
	resp.x_partner_old_code AS ma_cu_kh,
	(SELECT y.name FROM pos_order x, crm_customer_rank y WHERE x.invoice_id=ai.id AND x.x_rank_id=y.id) AS hang_kh_khi_mua_hang,
	to_char(ai.date_invoice, 'dd/mm/yyyy') AS ngay_hoa_don,
	ai.number AS so_hoa_don,
	ai.origin AS so_order, 
	ai.amount_total AS tien_hoa_don,
	ai.residual AS so_tien_no,
	ai.comment AS ghi_chu
FROM 
	account_invoice AS ai
	JOIN res_branch rb ON (ai.branch_id=rb.id)
	LEFT JOIN (
			SELECT name, x_partner_code, x_partner_old_code, id FROM res_partner
	) AS resp ON (ai.partner_id=resp.id)
WHERE 
	ai.state = 'open'
	AND ai.residual > 0
	AND ai.type='out_invoice'
	AND (ai.partner_id=ANY(STRING_TO_ARRAY(?, ',') :: INTEGER []) OR  ? = '0')
	AND (ai.branch_id=ANY(STRING_TO_ARRAY(?, ',') :: INTEGER []) OR  ? = '0')
	AND to_date(to_char(ai.date_invoice + INTERVAL '7 hours', 'dd/mm/yyyy'), 'dd/mm/yyyy') >= to_date(?, 'dd/mm/yyyy')
	AND to_date(to_char(ai.date_invoice + INTERVAL '7 hours', 'dd/mm/yyyy'), 'dd/mm/yyyy') <= to_date(?, 'dd/mm/yyyy')
ORDER BY
	resp.name;
	]]></xml-property>
