            <xml-property name="queryText"><![CDATA[SELECT
	code,
	SUM(doanh_so) AS DOANH_SO,
	SUM(CKTM) AS CKTM,
	SUM(CKVIP) AS CKVIP,
	SUM(COALESCE(CKCVCP,0)) AS CKCVCP,
	SUM(doanh_so-CKTM-CKVIP) AS DTSAUCK,
	SUM(doanh_so-CKTM-CKVIP-COALESCE(CKCVCP,0)) AS DOANH_THU
FROM (
	SELECT 
		DISTINCT po.name,
		ds.amount AS doanh_so,
		rb.code AS code,
-- 		to_date(to_char(date_order + INTERVAL '7 hours', 'dd/mm/yyyy'), 'dd/mm/yyyy') AS date_order,
		ap.amount_promotion AS CKTM,
		vip.vip AS CKVIP,
		COALESCE(pay_coupon.amount, 0) AS CKCVCP
	FROM
		pos_order AS po
		LEFT JOIN pos_order_line AS pol ON (pol.order_id=po.id)
		LEFT JOIN res_branch AS rb ON (rb.id=po.branch_id)
		LEFT JOIN (
				SELECT
					SUM(ROUND(price_unit*qty, 2)) AS amount,
					order_id
				FROM
					pos_order_line AS pol
				GROUP BY
					order_id
							) AS ds ON (ds.order_id=po.id) -- lấy ra doanh số = đơn giá x số lượng theo từng đơn POS
		LEFT JOIN (
				SELECT
					SUM(al.amount) AS amount,
					po.id AS id
				FROM
					pos_order AS po
					LEFT JOIN account_bank_statement_line AS al ON (al.pos_statement_id=po.id)
 					LEFT JOIN account_journal AS aj ON (aj.id=al.journal_id)
				WHERE
					aj.type = 'coupon'
				GROUP BY
					po.id
				) AS pay_coupon ON (pay_coupon.id=po.id) -- lấy ra số tiền thanh toán bằng coupon theo từng đơn POS
		LEFT JOIN (
				SELECT
					COALESCE(SUM(pol.x_amount_promotion),0) AS amount_promotion,
					order_id,
					po.name
				FROM
					pos_order_line AS pol
					LEFT JOIN pos_order AS po ON (po.id=pol.order_id)
				GROUP BY
					order_id, po.name
							) AS ap ON (ap.order_id=po.id) -- lấy ra tổng số tiền khuyến mại
		LEFT JOIN (
				SELECT
					SUM((pol.price_unit*pol.qty-COALESCE(pol.x_amount_promotion,0))*COALESCE(discount, 0)/100) AS vip, 
					order_id
				FROM
					pos_order_line AS pol
					LEFT JOIN pos_order AS po ON (po.id=pol.order_id)
				GROUP BY
					order_id
							) AS vip ON (vip.order_id=po.id) -- lấy ra tổng tiền chiết khấu vip
		
	WHERE
		po.state NOT IN ('draft', 'cancel') -- đặt trong các trạng thái của đơn là: done, invoice, paid
		AND to_date(to_char(po.date_order + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') >= to_date(?,'dd/mm/yyyy')
		AND to_date(to_char(po.date_order + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') <= to_date(?,'dd/mm/yyyy')
		AND (rb.id=ANY(string_to_array(?, ',')::INTEGER[]) or ?= '0')
		) AS a
GROUP BY
	code
ORDER BY
	code ASC]]></xml-property>
