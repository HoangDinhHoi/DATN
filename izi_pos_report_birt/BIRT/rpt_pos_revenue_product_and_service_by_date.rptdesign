            <xml-property name="queryText"><![CDATA[SELECT
	a.ten_cn,
	a.ma_cn,
	a.date_order date_order,
	SUM(doanh_thu_san_pham) doanh_thu_san_pham,
	SUM(doanh_thu_dich_vu) doanh_thu_dich_vu,
	SUM(doanh_thu_khac) doanh_thu_khac
FROM(
			SELECT
				rb.name ten_cn,
				rb.code ma_cn,
				(po.date_order + INTERVAL '7 hours') :: DATE date_order,
				(pol.x_revenue_rate*doanh_thu.dt) doanh_thu_san_pham,
				0 doanh_thu_dich_vu,
				0 doanh_thu_khac
			FROM
				pos_order po
				JOIN res_branch rb ON (po.branch_id=rb.id)
				JOIN pos_order_line pol ON (pol.order_id=po.id)
				JOIN product_product pp ON (pol.product_id=pp.id)
				JOIN product_template pt ON (pp.product_tmpl_id=pt.id)
				JOIN (
					SELECT
						SUM(al.amount) dt,
						po.id id
					FROM 
						pos_order po
						JOIN account_bank_statement_line al ON (al.pos_statement_id=po.id)
						JOIN account_journal aj ON (al.journal_id=aj.id)
					WHERE
						aj.type <> 'coupon'
					GROUP BY
						po.id
				) doanh_thu ON (doanh_thu.id=po.id)
			WHERE	
				pt.type IN ('product', 'consu')
				AND (rb.id = ANY(string_to_array(?, ',') :: INTEGER[]) or ? = '0') 
				AND po.state IN ('invoiced', 'paid', 'done')
				AND (po.date_order + INTERVAL '7 hours')::DATE >= to_date(?, 'dd/mm/yyyy')
				AND (po.date_order + INTERVAL '7 hours')::DATE <= to_date(?, 'dd/mm/yyyy')
			
		UNION ALL
		
			SELECT
				rb.name ten_cn,
				rb.code ma_cn,
				(po.date_order + INTERVAL '7 hours')::DATE date_order,
				0 doanh_thu_san_pham,
				(CASE WHEN po.name LIKE 'ES%' THEN pol.price_subtotal_incl ELSE (pol.x_revenue_rate*doanh_thu.dt) END ) doanh_thu_dich_vu,
				0 doanh_thu_khac
			FROM
				pos_order po
				JOIN res_branch rb ON (po.branch_id=rb.id)
				JOIN pos_order_line pol on (pol.order_id=po.id)
				JOIN product_product pp ON (pol.product_id=pp.id)
				JOIN product_template pt ON (pp.product_tmpl_id=pt.id)
				JOIN (
					SELECT
						SUM(al.amount) dt,
						po.id id
					FROM 
						pos_order po
						JOIN account_bank_statement_line al ON (al.pos_statement_id=po.id)
						JOIN account_journal aj ON (al.journal_id=aj.id)
					WHERE
						aj.type <> 'coupon'
					GROUP BY
						po.id
				) doanh_thu ON (doanh_thu.id=po.id)
			WHERE	
				pt.type = 'service'
				AND (rb.id = ANY(string_to_array(?, ',') :: INTEGER[] ) or ? = '0') 
				AND po.state IN ('invoiced', 'paid', 'done')
				AND (po.date_order + INTERVAL '7 hours')::DATE >= to_date(?, 'dd/mm/yyyy')
				AND (po.date_order + INTERVAL '7 hours')::DATE <= to_date(?, 'dd/mm/yyyy')
		
		UNION ALL
		
			SELECT
				rb.name ten_cn,
				rb.code ma_cn,
				(so.confirmation_date + INTERVAL '7 hours')::DATE date_order,
				sol.price_subtotal doanh_thu_san_pham,
				0 doanh_thu_dich_vu,
				0 doanh_thu_khac
			FROM
				sale_order so
				JOIN res_branch rb ON (so.branch_id=rb.id)
				JOIN sale_order_line sol ON (sol.order_id=so.id)
				JOIN product_product pp ON (sol.product_id=pp.id)
				JOIN product_template pt ON (pp.product_tmpl_id=pp.id)
			WHERE
				pt.type IN ('product', 'consu')
				AND (rb.id = ANY(string_to_array(?, ',') :: INTEGER[] ) or ?='0')
				AND (so.confirmation_date + INTERVAL '7 hours')::date >= to_date(?, 'dd/mm/yyyy')
				AND (so.confirmation_date + INTERVAL '7 hours')::date <= to_date(?, 'dd/mm/yyyy')
		
		UNION ALL
		
			SELECT
				rb.name ten_cn,
				rb.code ma_cn,
				(so.confirmation_date + INTERVAL '7 hours')::DATE date_order,
				0 doanh_thu_san_pham,
				sol.price_subtotal doanh_thu_dich_vu,
				0 doanh_thu_khac
			FROM
				sale_order so
				JOIN res_branch rb ON (so.branch_id=rb.id)
				JOIN sale_order_line sol ON (sol.order_id=so.id)
				JOIN product_product pp ON (sol.product_id=pp.id)
				JOIN product_template pt ON (pp.product_tmpl_id=pp.id)
			WHERE
				pt.type = 'service'
				AND (rb.id = ANY(string_to_array(?, ',') :: INTEGER[] ) or ?='0')
				AND (so.confirmation_date + INTERVAL '7 hours')::DATE >= to_date(?, 'dd/mm/yyyy')
				AND (so.confirmation_date + INTERVAL '7 hours')::DATE <= to_date(?, 'dd/mm/yyyy')
				
		UNION ALL
			
			SELECT
				rb.name ten_cn,
				rb.code ma_cn,
				rd.date_order date_order,
				rd.revenue_by_product doanh_thu_san_pham,
				rd.revenue_by_service doanh_thu_dich_vu,
				rd.other_revenue doanh_thu_khac
			FROM
				revenue_product_and_service_by_date rd
				JOIN pos_order po ON (po.id=rd.order_id)
				JOIN res_branch rb ON (rb.id=po.branch_id)
			) a
GROUP BY
	a.date_order, a.ten_cn, a.ma_cn
ORDER BY
	a.date_order;]]></xml-property>
