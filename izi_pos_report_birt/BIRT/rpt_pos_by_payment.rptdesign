            <xml-property name="queryText"><![CDATA[-- HOIHD--
SELECT  
	a.name AS session_name,
	to_char(d.date,'dd/mm/yyyy') AS date,
	(SELECT rb.name FROM res_branch rb, crm_team ct WHERE rb.id = ct.x_branch_id AND ct.id = b.crm_team_id) AS shop,
	(SELECT rb.name FROM res_branch rb, crm_team ct WHERE rb.id = ct.x_branch_id AND ct.id = b.crm_team_id) AS shop_code,
    (SELECT d.name AS journal_name FROM account_journal d WHERE d.id = c.journal_id),
    (SELECT g.name AS cus_name FROM res_partner g WHERE g.id = d.partner_id),
    (SELECT g.x_partner_old_code AS cus_code FROM res_partner g WHERE g.id = d.partner_id),
    (CASE
    	WHEN d.name like 'INV/%' THEN CONCAT('PAID_HMF', SPLIT_PART(d.ref, 'HMF', 2))
		WHEN d.name like 'RF_%' THEN SUBSTRING(d.name, 1, 18) 
		WHEN d.name like 'HMF%' THEN SUBSTRING(d.name, 1, 15)
		WHEN d.name like 'DEPOSIT%' THEN SUBSTRING(d.name, 1, 17)
		WHEN d.name like 'REFUND%' THEN SUBSTRING(d.name, 1, 16)
		ELSE d.name END ) AS order_name,
    d.amount,
    (SELECT 
    	SUM(st.amount) 
     FROM account_bank_statement_line AS st, account_journal AS j
     WHERE st.journal_id = j.id AND st.id = d.id) AS tt,
     d.note AS payment_ref
FROM 
	pos_session AS a 
	JOIN pos_config AS b ON a.config_id = b.id
    JOIN account_bank_statement AS c ON a.id = c.pos_session_id
    JOIN account_bank_statement_line AS d ON c.id = d.statement_id
    JOIN res_branch AS rb ON rb.id = a.branch_id
WHERE 
	(rb.id = ANY(string_to_array(?, ',') :: INTEGER []) OR ? = '0')
	AND to_date(to_char(d.date + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') >= to_date(?,'dd/mm/yyyy') 
	AND to_date(to_char(d.date + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') <= to_date(?,'dd/mm/yyyy')
ORDER BY 
	journal_name,
	d.date;]]></xml-property>
