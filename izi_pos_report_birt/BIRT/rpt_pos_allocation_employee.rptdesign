            <xml-property name="queryText"><![CDATA[SELECT 
	rb.code shop_code,
	j.name job_code,
	h.name employee_name,
    h.x_employee_code,
    SUM(d.amount_product) product, 
    SUM(d.amount_service) service, 
    SUM(d.amount_keep) keep,
    rb.name ten_chi_nhanh
FROM   
	pos_revenue_allocation a
	INNER JOIN pos_order b ON b.id = a.order_id
	INNER JOIN pos_revenue_allocation_line d ON a.id = d.revenue_allocation_id
	LEFT JOIN hr_employee h ON d.employee_id = h.id
	LEFT JOIN crm_team k ON k.id = b.x_team_id
	LEFT JOIN res_branch rb ON rb.id = k.x_branch_id
	LEFT JOIN hr_job j ON j.id = h.job_id
WHERE
	a.state = 'confirmed'
	AND (rb.id = ANY(string_to_array(?, ',') :: INTEGER [] ) OR ? = '0')
	AND to_date(to_char(b.date_order + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') >= to_date(?,'dd/mm/yyyy') 
	AND to_date(to_char(b.date_order + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') <= to_date(?,'dd/mm/yyyy')
GROUP BY 
	h.id,rb.code,j.name, rb.name
ORDER BY 
	rb.code,
	j.name DESC,
	h.name;]]></xml-property>
