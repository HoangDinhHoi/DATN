            <xml-property name="queryText"><![CDATA[SELECT 
	rb.code,
	rb.name,
	(SUM(d.amount_product) + SUM(d.amount_service)+SUM(d.amount_keep)) amount_total,
	sum(d.amount_product) amount_product,
	SUM(d.amount_service) amount_service, SUM(d.amount_keep) amount_keep
FROM 
	pos_revenue_allocation a 
JOIN 
	pos_order b on a.order_id = b.id
LEFT JOIN 
	res_branch rb on rb.id = b.branch_id
JOIN 
	pos_revenue_allocation_line d on d.revenue_allocation_id = a.id 
WHERE 
	a.state = 'confirmed'
	AND (rb.id = ANY(string_to_array(?, ',')::integer[]) or ? = '0')
	AND to_date(to_char(b.date_order + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') >= to_date(?,'dd/mm/yyyy') 
	AND to_date(to_char(b.date_order + INTERVAL '7 hours','dd/mm/yyyy'),'dd/mm/yyyy') <= to_date(?,'dd/mm/yyyy')
GROUP BY 
	rb.code,
	rb.name
ORDER BY 
	rb.code;]]></xml-property>
