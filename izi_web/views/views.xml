<odoo>
    <data>
        <template id="assets_backend" name="izi_web assets backend" inherit_id="web.assets_backend">
            <xpath expr="//link[@href='/web/static/src/scss/fields.scss']" position="replace">
                <link rel="stylesheet" type="text/scss" href="/izi_web/static/src/scss/fields.scss"/>
            </xpath>
        </template>
        <template id="assets_common" name="izi_web common assets" inherit_id="web.assets_common">
            <xpath expr="//link[@href='/web/static/src/scss/navbar.scss']" position="replace">
                <link rel="stylesheet" type="text/scss" href="/izi_web/static/src/scss/navbar.scss"/>
            </xpath>
        </template>
    </data>
</odoo>
