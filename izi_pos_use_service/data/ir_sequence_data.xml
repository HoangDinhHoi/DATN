<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_pos_use_service" model="ir.sequence">
            <field name="name">REDEEM</field>
            <field name="code">pos.use.service</field>
            <field name="prefix">REDEEM%(y)s%(month)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="sequence_use_service_card_yc" model="ir.sequence">
             <field name="name">YC/NVL</field>
             <field name="code">pos.material.request</field>
             <field name="prefix">PXKHL/%(y)s%(month)s/</field>
             <field name="padding">4</field>
             <field name="number_next">1</field>
             <field name="number_increment">1</field>
             <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>