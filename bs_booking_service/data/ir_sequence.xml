<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_sequence_service_bed_code" model="ir.sequence">
            <field name="name">Service Bed Code</field>
            <field name="code">service.bed.code</field>
            <field name="prefix">BED%(y)s</field>
            <field name="padding">2</field>
        </record>

        <record id="ir_sequence_booking_service_code" model="ir.sequence">
            <field name="name">Booking Service Code</field>
            <field name="code">booking.service.code</field>
            <field name="prefix">BOOKING%(y)s%(month)s/</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
