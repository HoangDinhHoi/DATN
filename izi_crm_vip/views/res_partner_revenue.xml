<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_partner_revenue_view_form" model="ir.ui.view">
            <field name="name">Partner revenue</field>
            <field name="model">res.partner.revenue</field>
            <field name="arch" type="xml">
                <form string="Partner revenue">
                    <sheet>
                        <group>
                            <group>
                                <field name="revenue"/>
                                <field name="partner_id"/>
                            </group>
                            <group>
                                <field name="revenue_date_view"/>
                                <field name="revenue_date"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>